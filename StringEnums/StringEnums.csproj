<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/dshe/StringEnums</PackageProjectUrl>
    <PackageIconUrl>https://github.com/dshe.png</PackageIconUrl>
    <PackageTags>string enum enumeration type-safe</PackageTags>
    <Description>A simple and flexible alternative to System.Enum based on strings.</Description>
    <Authors>DavidS</Authors>
    <Version>1.0.0</Version>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
  </PropertyGroup>

</Project>
